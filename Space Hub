local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local Title = Title
local LoadingText = LoadingText
local LoadingIcon = LoadingIcon
local Version = Version
local KeyFrme = KeyFrme
local KeyInput = KeyInput
local VerifyBtn = VerifyBtn

local CorrectKey = Key

local function tween(obj, time, props)
	return TweenService:Create(
		obj,
		TweenInfo.new(time, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
		props
	)
end

Title.Visible = true
Title.Size = UDim2.new(0,0,0,0)
Title.Position = UDim2.new(0.5,0,0.5,0)

tween(Title, 0.8, {
	Size = UDim2.new(0.55,0,0.1,0)
}):Play()

task.wait(0.9)

tween(Title, 0.6, {
	Position = UDim2.new(0.5,0,0.25,0)
}):Play()

task.wait(0.7)

LoadingText.Visible = true
LoadingIcon.Visible = true
Version.Visible = true

task.spawn(function()
	while LoadingIcon.Visible do
		LoadingIcon.Rotation = 0
		tween(LoadingIcon, 1.5, {Rotation = 360}):Play()
		task.wait(1.5)
	end
end)

local function animatedText(baseText, duration)
	local start = tick()
	local dots = {".", ". .", ". . ."}
	local i = 1
	while tick() - start < duration do
		LoadingText.Text = baseText .. " " .. dots[i]
		i = i % #dots + 1
		task.wait(0.5)
	end
end

animatedText("Loading Script Content", 5)
animatedText("Preparing KeySystem", 3)

Title.Visible = false
LoadingText.Visible = false
LoadingIcon.Visible = false
Version.Visible = false

KeyFrme.Visible = true
KeyFrme.Size = UDim2.new(0,0,0,0)

tween(KeyFrme, 1, {
	Size = UDim2.new(0.48,0,0.48,0)
}):Play()

VerifyBtn.MouseButton1Click:Connect(function()
	local input = KeyInput.Text

	if input == CorrectKey then

		KeyInput.Text = ""
		KeyInput.PlaceholderColor3 = Color3.fromRGB(0,255,0)
		KeyInput.PlaceholderText = "Verification Success"

		task.wait(2)

		KeyFrme.Visible = false
		LoadingText.Visible = true
		LoadingText.TextColor3 = Color3.fromRGB(255,255,255)
		LoadingText.Size = UDim2.new(0.48,0,0.12,0)

		animatedText(
			"Verification Completed, Waiting for space hub response",
			5
		)
		
        SpaceHubKeySystem:Destroy()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/devsmiley245/The-Space-Simulator-Script/refs/heads/main/Space%20Hub%20Pass%20Gate"))()
		
	else
		KeyInput.Text = ""
		KeyInput.PlaceholderColor3 = Color3.fromRGB(255,0,0)
		KeyInput.PlaceholderText = "Invalid Key, Join discord for key"

		task.wait(3)

		KeyInput.PlaceholderColor3 = Color3.fromRGB(178, 178, 178)
		KeyInput.PlaceholderText = "Get The Key from discord"
	end
end)
